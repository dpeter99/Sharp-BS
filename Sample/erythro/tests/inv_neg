char* str;
int x;

int :: main() {
    x = -23;
    PrintInteger(x);

    x = 1;
    x = ~x;
    PrintInteger(x);

    x = 2 > 5;
    PrintInteger(x);

    x = !x;
    PrintInteger(x);

    x = !x;
    PrintInteger(x);

    x = 13;
    if(x) {
        PrintInteger(13);
    }

    x = 0;
    if(!x) {
        PrintInteger(14);
    }

    for(str = "Hello World\n"; *str; str++) {
        PrintChar(*str);
    }

    return (0);
}